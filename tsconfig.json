{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@tools/*": [
        "app/modules/tools/*"
      ],
      "@modules": [
        "app/modules"
      ],
      "@modules/*": [
        "app/modules/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@components": [
        "app/components"
      ],
      "@directives": [
        "app/directives"
      ],
      "@directives/*": [
        "app/directives/*"
      ],
      "@models": [
        "app/models"
      ],
      "@models/*": [
        "app/models/*"
      ],
      "@services/*": [
        "app/services/*"
      ],
      "@services": [
        "app/services"
      ],
      "@store/*": [
        "app/store/*"
      ]
    }
  }
}
