{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@tools/*": [
        "app/modules/tools/*"
      ],
      "@shapes/*": [
        "app/modules/tools/shapes/*"
      ],
      "@shapes": [
        "app/modules/tools/shapes"
      ],
      "@tools": [
        "app/modules/tools"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@components": [
        "app/components"
      ],
      "@directives": [
        "app/directives"
      ],
      "@directives/*": [
        "app/directives/*"
      ],
      "@services/*": [
        "app/services/*"
      ],
      "@services": [
        "app/services"
      ],
      "@store/*": [
        "app/store/*"
      ],
      "@store": [
        "app/store"
      ],
      "@math/*": [
        "app/math/*"
      ],
      "@math": [
        "app/math"
      ],
      "@rx": [
        "app/libs/rx"
      ]
    }
  }
}
